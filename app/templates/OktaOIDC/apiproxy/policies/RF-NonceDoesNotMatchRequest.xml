<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-NonceDoesNotMatchRequest">
    <DisplayName>RF-NonceDoesNotMatchRequest</DisplayName>
    <FaultRules/>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <Payload contentType="application/json">{
                "error" : {
                    "code" : 400,
                    "message" : "Nonce does not match authentication request"
                }
            }
            </Payload>
            <StatusCode>400</StatusCode>
            <ReasonPhrase>Nonce does not match authentication request</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
