<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="SetConfigVariables">
    <DisplayName>SetConfigVariables</DisplayName>
    <FaultRules/>
    <Properties/>
    <AssignVariable>
        <Name>okta.org</Name>
        <Value>OKTAORGNAME</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>okta.auth_server_id</Name>
        <Value>OKTAAUTHSERVERID</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>okta.issuer</Name>
        <Value>https://OKTAORGNAME/oauth2/OKTAAUTHSERVERID</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>okta.client_id</Name>
        <Value>OKTACLIENTID</Value>
    </AssignVariable>
    <AssignVariable>
        <Name>okta.client_secret</Name>
        <Value>OKTACLIENTSECRET</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
</AssignMessage>
